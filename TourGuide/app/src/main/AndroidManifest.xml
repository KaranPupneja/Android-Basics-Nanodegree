<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ayaan.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".waterfall" android:label="Waterfalls"/>
        <activity android:name=".hotels" android:label="Hotels"/>
        <activity android:name=".attractions" android:label="Attractions"/>
        <activity android:name=".restaurants" android:label="Restaurants"></activity>
    </application>

</manifest>